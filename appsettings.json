{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CacheDirectory": "./cache",
  "CacheRetentionHours": 24,
  "CacheExpirationMinutes": 12.5,
  "CacheManagement": {
    "CheckIntervalMinutes": 5,
    "InitialDelaySeconds": 10,
    "UpdateStaggerSeconds": 2,
    "LocationStaggerSeconds": 1
  },
  "CacheCleanup": {
    "IntervalHours": 1
  },
  "Screenshot": {
    "DynamicContentWaitMs": 2000,
    "TileRenderWaitMs": 5000
  },
  "Debug": {
    "Enabled": false,
    "WaitMs": 2000
  },
  "Timezone": "Australia/Brisbane"
}
